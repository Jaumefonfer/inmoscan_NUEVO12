{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "fronted/index.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "fronted/dist",
        "framework": "none"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "fronted/dist/$1"
    }
  ]
}